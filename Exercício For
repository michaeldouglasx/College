estoque = [200, 53 , 12 , 350 , 23, 200, 145, 43, 23,]
produtos = ['coca', 'fanta', 'heineken', 'skol', 'sukita', 'brahma', 'mineiro', 'guaraná' , 'monster']
qtd_minimo = 50
for i, estoqueminimo in enumerate (estoque):
    if estoqueminimo > qtd_minimo:
        print (f'A quantidade de estoque: {estoqueminimo}, é suficiente! ')
    else:
        print(f'A quantidade de estoque: {estoqueminimo}, não é suficiente!')
    